{
  "name": "@hit/feature-pack-vault",
  "version": "1.0.69",
  "description": "Password management and 2FA vault with team sharing, folders, CSV import, and TOTP",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./pages/*": {
      "types": "./dist/pages/*.d.ts",
      "import": "./dist/pages/*.js",
      "require": "./dist/pages/*.js",
      "default": "./dist/pages/*.js"
    },
    "./schema": {
      "types": "./dist/schema/vault.d.ts",
      "import": "./dist/schema/vault.js",
      "require": "./dist/schema/vault.js",
      "default": "./dist/schema/vault.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.js",
      "default": "./dist/hooks/index.js"
    },
    "./server/api/*": {
      "types": "./dist/server/api/*.d.ts",
      "import": "./dist/server/api/*.js",
      "require": "./dist/server/api/*.js",
      "default": "./dist/server/api/*.js"
    },
    "./server/utils/*": {
      "types": "./dist/server/utils/*.d.ts",
      "import": "./dist/server/utils/*.js",
      "require": "./dist/server/utils/*.js",
      "default": "./dist/server/utils/*.js"
    }
  },
  "files": [
    "dist",
    "feature-pack.yaml"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "peerDependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hit/sdk": "*",
    "@hit/ui-kit": "*",
    "drizzle-orm": "^0.38.0",
    "drizzle-zod": ">=0.7.0",
    "lucide-react": "^0.460.0",
    "next": "^15.5.9",
    "react": "^19.2.3",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hit/sdk": "git+https://github.com/c0x65o/hit-sdk-typescript.git#8b77bff5b14ca3541a4e93830cbb913ff58331d6",
    "@hit/ui-kit": "github:c0x65o/hit-ui-kit#4237e657aed7de52c31642dd5c27d0cc4bcbc9cf",
    "@types/node": "^20",
    "@types/react": "^19.0.0",
    "drizzle-orm": "^0.38.0",
    "drizzle-zod": "^0.7.0",
    "lucide-react": "^0.460.0",
    "next": "^15.5.9",
    "typescript": "^5",
    "zod": "^4.0.0"
  },
  "keywords": [
    "hit",
    "feature-pack",
    "vault",
    "password-manager",
    "2fa",
    "totp",
    "sms",
    "security",
    "ui-kit"
  ],
  "license": "MIT"
}
